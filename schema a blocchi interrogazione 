   "source": [
    "print(\"dammi il numero n a cui vuoi arrivare\")\n",
    "n = int(input())\n",
    "for i in range(1, n + 1, 1):\n",
    "    print(\"Calcolo il quadrato\")\n",
    "    quadrato = i ** 2\n",
    "    print(quadrato)"
